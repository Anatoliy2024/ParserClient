{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "function/getInfoFromServer.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/function/(.*)",
      "dest": "/function/getInfoFromServer.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
